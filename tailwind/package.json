{
  "name": "@hyva-themes/hyva-eds-demo",
  "version": "3.0.0",
  "description": "Hyvä EDS Integration Demo.",
  "main": "tailwind.config.js",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "browser-sync": "^2.27.10",
    "postcss": "^8.4.33",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^9.3.0",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "build": "npx tailwindcss --postcss -i tailwind-source.css -o ../styles/styles.css --minify",
    "dev": "npx tailwindcss --postcss -i tailwind-source.css -o ../styles/styles.css --watch --verbose",
    "watch": "npm run dev",
    "start": "npm run dev"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "browserslist": [
    "last 3 version",
    "> 0.5%",
    "not dead",
    "not op_mini all"
  ],
